@model Project1.Models.Session
<p>Create a new session.</p>
<form asp-controller="Coach" asp-action="AddSession" method="post">
    <input asp-for="SessionId" type="hidden"/>
    <input asp-for="CoachId" type="hidden"/>
    <label asp-for="StartDate"></label>
    <input asp-for="StartDate" />
    <br />
    <label asp-for="EndDate"></label>
    <input asp-for="EndDate" />
    <br />
    <label asp-for="SeatCapacity"></label>
    <input asp-for="SeatCapacity" />
    <br />
    <label asp-for="DailyStartTime"></label>
    <input asp-for="DailyStartTime" />
    <br />
    <button type="submit">Add Session</button>
</form>